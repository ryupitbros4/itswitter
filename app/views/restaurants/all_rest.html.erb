<div role="tabpanel">
  <ul class="nav nav-tabs" role="tablist">
    <% @gozyuuonn.each_with_index do |aiu,index| %>
    <% if index == 0 %> <li class="active" role="presentation"> <% else %> <li role="presentation"> <% end %>
      <a aria-controls="tab_<%= aiu %>"  data-toggle="tab" href="#tab_<%= aiu %>" role="tab"><font size="5"><%= aiu %></font></a>
    </li>
    <% end %>
  </ul>

  <div class="tab-content">
    <% @gozyuuonn.each_with_index do |aiu,index| %>
    <% if index == 0 %> <div class="tab-pane fade in active" id="tab_<%= aiu %>" role="tabpanel"> <% else %> <div class="tab-pane fade" id="tab_<%= aiu %>" role="tabpanel"> <% end %>
	
	<ul class="nav nav-tabs" role="tablist">
          <% @gozyuuonn2[index][0..-1].each_with_index do |aiu2,index2| %>
          <% if index2 == 0 %> <li class="active" role="presentation"> <% else %> <li role="presentation"> <% end %>
            <a aria-controls="tab_<%= aiu2 %>-2"  data-toggle="tab" href="#tab_<%= aiu2 %>-2" role="tab"><%= aiu2 %></a>
          </li>
          <% end %>
	</ul>
	
        <div class="tab-content">
          <% @gozyuuonn2[index][0..-1].each_with_index do |aiu2,index2| %>
          <% if index2 == 0 %> <div class="tab-pane fade in active" id="tab_<%= aiu2 %>-2" role="tabpanel"> <% else %> <div class="tab-pane fade" id="tab_<%= aiu2 %>-2" role="tabpanel"> <% end %>
	      <%= render partial: '/restaurant_card', locals: { restaurants: @restaurants.initial_search(aiu2), crowded_image: @crowded_image } %>
	      
              <% if (index == 1 or index == 2 or index == 3 or index == 5) %>
              <%= render partial: '/restaurant_card', locals: { restaurants: @restaurants.initial_search(@gozyuuonnd[index - 1][index2]), crowded_image: @crowded_image } %>
              <% if (index == 5) %>
              <%= render partial: '/restaurant_card', locals: { restaurants: @restaurants.initial_search(@gozyuuonnd[3][index2]), crowded_image: @crowded_image } %>
              <% end %>
              <% end %>
	      
            </div>
            <% end %>
          </div>
	</div>
	<% end %>
      </div>
    </div>
